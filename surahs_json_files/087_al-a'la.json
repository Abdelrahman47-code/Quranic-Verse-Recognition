{
    "surah": "الأعلى",
    "number": 87,
    "ayahs": [
        {
            "ayah_number": 1,
            "text": "سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى"
        },
        {
            "ayah_number": 2,
            "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ"
        },
        {
            "ayah_number": 3,
            "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ"
        },
        {
            "ayah_number": 4,
            "text": "وَٱلَّذِيٓ أَخْرَجَ ٱلْمَرْعَىٰ"
        },
        {
            "ayah_number": 5,
            "text": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ"
        },
        {
            "ayah_number": 6,
            "text": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ"
        },
        {
            "ayah_number": 7,
            "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ"
        },
        {
            "ayah_number": 8,
            "text": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ"
        },
        {
            "ayah_number": 9,
            "text": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ"
        },
        {
            "ayah_number": 10,
            "text": "سَيَذَّكَّرُ مَن يَخْشَىٰ"
        },
        {
            "ayah_number": 11,
            "text": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى"
        },
        {
            "ayah_number": 12,
            "text": "ٱلَّذِي يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ"
        },
        {
            "ayah_number": 13,
            "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"
        },
        {
            "ayah_number": 14,
            "text": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ"
        },
        {
            "ayah_number": 15,
            "text": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ"
        },
        {
            "ayah_number": 16,
            "text": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا"
        },
        {
            "ayah_number": 17,
            "text": "وَٱلْأٓخِرَةُ خَيْرٞ وَأَبْقَىٰٓ"
        },
        {
            "ayah_number": 18,
            "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلْأُولَىٰ"
        },
        {
            "ayah_number": 19,
            "text": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ"
        }
    ]
}
